{
  "name": "framer-webview",
  "version": "0.0.1",
  "description": "Web view module for Framer.js",
  "main": "index.js",
  "devDependencies": {
    "babelify": "^6.0.2",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "coffeeify": "^1.1.0",
    "framerify": "peteschaffner/framerify",
    "garnish": "^2.1.3"
  },
  "scripts": {
    "start": "NODE_PATH=./modules budo index.js:bundle.js --live --port 3000 -- --transform coffeeify --transform babelify --require ./index.js:framer-webview --extension .coffee | garnish"
  },
  "keywords": [
    "prototyping",
    "framerjs"
  ],
  "author": "pjschaffner@gmail.com",
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify",
      "framerify"
    ]
  }
}
